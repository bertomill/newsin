{
  "functions": {
    "app/assistant/api/route.ts": {
      "maxDuration": 60
    },
    "app/assistant/api/stream/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "regions": ["iad1"],
  "crons": []
} 